<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--v. a.b.c:
    a {uint} - Very significant update(for example, adding a new features,
    fundamental refactoring, e.t.c),
    b {uint} - Error correction usually,
    c {uint} - Minor internal lightsearch.server changes.
    -->
    <bean id="appGreetingsDefault" class="lightsearch.server.about.AppGreetingsDefaultImpl">
        <constructor-arg index="0" type="java.lang.String" value="&#10;------------------------------------------------&#10;LightSearch Server, version 2.8.1&#10;Welcome!&#10;------------------------------------------------"/>
    </bean>

    <bean id="endStartUpMessageDefault" class="lightsearch.server.about.EndStartupMessageDefaultImpl">
        <constructor-arg index="0" type="java.lang.String" value="Now Logging is in the folder logs and this window. For administration use LightSearch Admin Panel."/>
    </bean>

</beans>